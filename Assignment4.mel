proc Calculator(float $numbers[], int $operation)
{
    
    if ($operation == 1)
    {
        Sum($numbers);
    }
    else if ($operation == 2)
    {
        Difference($numbers);
    }
    else if ($operation == 3)
    {
        Multiplication($numbers);
    }
    else if ($operation == 4)
    {
        Division($numbers);
    }
    /*else if ($operation == 5)
    {
        Power($numbers);
    }
    else if ($operation == 6)
    {
        Mean($numbers);
    }*/
    else
    {
        print("Please enter a valid operation (Numbers 1 - 6).\n");
    }

}

Calculator({2,3,5}, 2);