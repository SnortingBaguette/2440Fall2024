global proc Sum(float $numbers[])
{
    float $result;
    int $size = size($numbers);
    for($i = 0; $i < $size; $i++)
    {
        $result += $numbers[$i];
    }
    print($result);
}

global proc Difference(float $numbers[])
{
    float $result;
    int $size = size($numbers);
    for($i = 0; $i < $size; $i++)
    {
        $result -= $numbers[$i];
    }
    print($result);
}

global proc Multiplication(float $numbers[])
{
    float $result;
    int $size = size($numbers);
    for($i = 1; $i < $size; $i++)
    {
        $result *= $numbers[$i];
    }
    print($result);
}

global proc Division(float $numbers[])
{
    float $result;
    int $size = size($numbers);
    for($i = 1; $i < $size; $i++)
    {
        $result /= $numbers[$i];
    }
    print($result);
}

